{
  "name": "llm-orchestrator",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "start": "bun run src/main.ts",
    "dev": "bun run --hot src/main.ts",
    "server:mcp": "bun run src/server.ts --mode=mcp",
    "server:webhook": "bun run src/server.ts --mode=webhook",
    "build": "bun build ./src/main.ts --compile --outfile dist/llm-orchestrator",
    "test": "bun test",
    "test-cli": "bun run src/test-cli.ts",
    "typecheck": "tsc --noEmit",
    "lint": "bunx biome check src/",
    "generate-bun-nix": "bunx bun2nix"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.24.3",
    "@qdrant/js-client-rest": "^1.16.2",
    "openai": "^6.10.0",
    "pino": "^10.1.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/bun": "^1.3.4",
    "pino-pretty": "^13.1.3",
    "typescript": "^5.9.3"
  }
}
